function testAtmosphereModel()
altitudes = linspace(300000, 0, 1000);
altitudes = altitudes(:);
densities = zeros(0, length(altitudes));
%densities = densities(:);

for i = 1:length(altitudes)
    densities(i) = marsAtmosphere(altitudes(i));
end

disp(altitudes)
disp('Densities');
disp(densities)

%T = table(altitudes, densities);
%disp(T);

semilogx(densities, altitudes./1000);
xlabel('Density (kg / m^3)')
ylabel('Height (km)')

end